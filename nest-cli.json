{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/mav-housing-config/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/mav-housing-config/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/mav-housing-config",
  "projects": {
    "auth-server": {
      "type": "application",
      "root": "apps/auth-server",
      "entryFile": "main",
      "sourceRoot": "apps/auth-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-server/tsconfig.app.json"
      }
    },
    "comms-server": {
      "type": "application",
      "root": "apps/comms-server",
      "entryFile": "main",
      "sourceRoot": "apps/comms-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/comms-server/tsconfig.app.json"
      }
    },
    "internal-api": {
      "type": "application",
      "root": "apps/internal-api",
      "entryFile": "main",
      "sourceRoot": "apps/internal-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/internal-api/tsconfig.app.json"
      }
    },
    "mav-housing-config": {
      "type": "application",
      "root": "apps/mav-housing-config",
      "entryFile": "main",
      "sourceRoot": "apps/mav-housing-config/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mav-housing-config/tsconfig.app.json"
      }
    },
    "public-api": {
      "type": "application",
      "root": "apps/public-api",
      "entryFile": "main",
      "sourceRoot": "apps/public-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/public-api/tsconfig.app.json"
      }
    }
  }
}